<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

<xs:element name = "accountStatement">
    <xs:complexType>
        <xs:sequence>
            <xs:element name = "summaryHeader" minOccurs = "1" maxOccurs = "1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="accountSummary" minOccurs="1" maxOccurs="1">
                            <xs:complexType>
                                <xs:attribute name="summary" use="required">
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                            <xs:enumeration value="Checking Summary"/>
                                            <xs:enumeration value="Savings Summary"/>     
                                            <xs:enumeration value="Certificate of Deposit Summary"/> 
                                            <xs:enumeration value="Money Market Account Summary"/>
                                            <xs:enumeration value="Individual Retirement Accounts  Summary"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name = "accountTypeDesc" minOccurs="1" maxOccurs="1">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name = "desc" minOccurs = "1" maxOccurs ="1" type= "xs:string"/>
                                </xs:sequence>
                            <xs:attribute name = "accountType" use = "optional"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name ="balanceBreakdown" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="balanceBreakdownHeader" maxOccurs= "1" type="xs:string"/>
                        <xs:element name = "beginningBalance" minOccurs="1" maxOccurs = "1" type="xs:string"/>
                        <xs:element name = "beginningBalanceAmount" minOccurs="1" maxOccurs = "1" type="xs:decimal"/>
                        <xs:element name = "deposits" maxOccurs = "1" type="xs:string"/>
                        <xs:element name = "depositAmount" maxOccurs = "1">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="deposit" minOccurs = "1" maxOccurs ="1">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:decimal">
                                            <xs:minInclusive value="0"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>   
                        </xs:element>
                        <xs:element name = "physicalWithdrawals" maxOccurs = "1" type="xs:string"/>
                        <xs:element name = "physicalWithdrawalsAmount" maxOccurs = "1">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="withdrawal" minOccurs = "1" maxOccurs ="1">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:decimal">
                                            <xs:maxInclusive value="0"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name = "electronicWithdrawals" maxOccurs = "1" type="xs:string"/>
                        <xs:element name = "electronicWithdrawalsAmount" maxOccurs = "1">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="withdrawal" minOccurs = "1" maxOccurs ="1">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:decimal">
                                            <xs:maxInclusive value="0"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>                            
                        </xs:element>
                        <xs:element name = "endingBalance" minOccurs="1" maxOccurs = "1" type="xs:string"/>
                        <xs:element name = "endingBalanceAmount" minOccurs="1" maxOccurs = "1" type="xs:decimal"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name = "noteToMember" minOccurs = "1" maxOccurs ="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name = "note" minOccurs = "1" maxOccurs = "1" type="xs:string"/>
                    </xs:sequence>
                    <xs:attribute name="noteType" use="required"/>
                </xs:complexType>
            </xs:element>

            <xs:element name = "transactions" minOccurs = "1" maxOccurs = "1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name = "headersList" maxOccurs= "unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name = "tableHeader" maxOccurs="unbounded" type="xs:string"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name = "transBeginningBalance" minOccurs="1" maxOccurs = "1" type="xs:string"/>
                        <xs:element name = "transBeginningBalanceAmount" minOccurs="1" maxOccurs = "1" type="xs:string"/>

                        <xs:element name = "transactionTable" minOccurs="1" maxOccurs = "1">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name = "row" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name = "date" minOccurs="1" maxOccurs = "1" type = "xs:string"/>
                                                <xs:element name = "description" minOccurs="1" maxOccurs = "1" type = "xs:string"/>
                                                <xs:element name = "amount" minOccurs="1" maxOccurs = "1" type = "xs:string"/>
                                                <xs:element name = "balance" minOccurs="1" maxOccurs = "1" type = "xs:string"/>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name = "transEndingBalance" minOccurs="1" maxOccurs = "1" type = "xs:string"/>
                        <xs:element name = "transEndingBalanceAmount" minOccurs="1" maxOccurs = "1" type = "xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="currency" use="required"/>
        <xs:attribute name="language" use="optional" default = "EN"/>
    </xs:complexType>
</xs:element>
</xs:schema>